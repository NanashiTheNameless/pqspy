{
  "manifest_version": 2,
  "name": "PQSpy-NamelessNanashi",
  "description": "NamelessNanashi's version of PQSpy: Shows whether the connection was protected with post-quantum encryption",
  "version": "0.3.0.1",
  "homepage_url": "https://github.com/NanashiTheNameless/pqspy",
  "browser_action": {
    "default_icon": {
      "32": "icons/unk.png"
    },
    "default_title": "PQSpy",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "webRequest", "webRequestBlocking", "<all_urls>"],
  "background": {
    "scripts": [ "background.js" ]
  },
  "icons": {
    "32": "icons/yes.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{72afe9c3-92ea-45d8-bf5e-9a62198192a9}",
      "data_collection_permissions": {
        "required": [
          "none"
        ]
      },
      "strict_min_version": "79.0"
    }
  }
}
